{
    "name": "koschos/php-retry",
    "description": "PHP retry library which is a port of the spring retry",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "./bin/"
    },
    "autoload-dev": {
        "psr-4": {
            "Koschos\\Retry\\": "src/retry",
            "Koschos\\Retry\\Tests\\": "tests/retry",
            "Koschos\\Classify\\": "src/classify",
            "Koschos\\Classify\\Tests\\": "tests/classify"
        }
    },
    "require": {
        "php": "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "7"
    }
}
